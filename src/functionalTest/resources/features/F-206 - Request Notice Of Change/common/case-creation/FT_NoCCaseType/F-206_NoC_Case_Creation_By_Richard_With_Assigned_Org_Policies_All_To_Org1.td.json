            "OrganisationID": "${[scenarioContext][childContexts][Get_Organisation_Identifier][testData][actualResponse][body][organisationIdentifier]}",
            "OrganisationName": "CCD Solicitors Limited"
          }
        },
        "otherPartyOrganisationPolicy": {
          "Organisation": {
            "OrganisationID": "${[scenarioContext][childContexts][Get_Organisation_Identifier][testData][actualResponse][body][organisationIdentifier]}",
            "OrganisationName": "CCD Solicitors Limited"
          }
        }
