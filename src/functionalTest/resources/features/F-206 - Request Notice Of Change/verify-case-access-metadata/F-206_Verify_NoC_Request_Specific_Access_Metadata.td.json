{
  "_guid_": "F-206_Verify_NoC_Request_Specific_Access_Metadata",
  "_extends_": "Retrieve_Access_Metadata_By_Case_ID__Base",

  "specs": [
    "to get Grant Access Metadata API returning Specific Grant Access for case"
  ],

  "users": {
    "invokingUser": {
      "_extends_": "BeftaMasterCaseworker"
    }
  },

  "request": {
    "pathVariables": {
      "caseId": "${[scenarioContext][parentContext][testData][request][body][case_id]}"
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body": {
      "accessGrants" : [ "SPECIFIC" ],
      "accessProcess": "NONE"
    }
  }
}
