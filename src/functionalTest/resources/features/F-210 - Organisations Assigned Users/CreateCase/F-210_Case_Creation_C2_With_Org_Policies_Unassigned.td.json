{
  "_guid_": "F-210_Case_Creation_C2_With_Org_Policies_Unassigned",
  "_extends_": "NoC_Case_Creation_With_Org_Policies__Base",

  "specs": [
    "by Richard to create a case - C2 - is not assigned to any orgs"
  ],

  "users": {
    "invokingUser": {
      "_extends_": "ACA_Users_Richard"
    }
  },

  "prerequisites": [
    {
      "Token_Creation": "F-210_Case_Creation_Token_Creation"
    }
  ],

  "request": {
    "body": {
      "event_token": "${[scenarioContext][childContexts][Token_Creation][testData][actualResponse][body][token]}",

      "data": {
        "applicantOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_OrgID_Empty"
        },
        "respondentOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_OrgID_Null"
        },
        "otherPartyOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_Null"
        }
      }
    }
  },

  "expectedResponse": {
    "body": {
      "case_data": {
        "applicantOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_OrgID_Empty"
        },
        "respondentOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_OrgID_Null"
        },
        "otherPartyOrganisationPolicy": {
          "_extends_" : "F-210_Organisation_Null"
        }
      }
    }
  }
}
